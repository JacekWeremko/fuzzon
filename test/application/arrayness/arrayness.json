{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "type": "object",
        "test_cases": {
            "id": "/properties/test_cases",
            "type": "integer"
        },
        "test_case": {
            "id": "/properties/test_case",
            "type": "array",
            "length": { "$ref": "/properties/test_cases" },
            "items": {
                "id": "/properties/test_case/items",
                "type": "object",
                "k": {
                    "id": "/properties/test_case/items/k",
                    "type": "integer"
                },
                "m": {
                    "id": "/properties/test_case/items/m",
                    "type": "integer"
                },
                "values": {
                    "id": "/properties/test_cases/items/values",
                    "type": "array",
                    "length": { "$ref": "/properties/test_case/items/k" },
                    "items": {
                        "id": "/properties/test_cases/items/values/items",
                        "type": "integer"
                    }
                }
            }
        }
    }
}
