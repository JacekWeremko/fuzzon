{
    "$schema": "http://json-schema.org/schema#",
    "id": "json schema example",
    "type": "object",
    "properties": {
        "type": "object",
        "test_cases": {
            "id": "/properties/test_cases",
            "type": "integer",
			"minimum": 1,
			"maximum": 3
        },
        "test_case": {
            "id": "/properties/test_case",
            "type": "array",
            "length": { "$ref": "/properties/test_cases" },
            "items": {
                "id": "/properties/test_case/items",
                "type": "object",
                "properties": {
                    "id": "/properties/test_case/items/properties",
                    "type": "object",
                    "k": {
                        "id": "/properties/test_case/items/properties/k",
                        "type": "integer"
                    },
                    "m": {
                        "id": "/properties/test_case/items/properties/m",
                        "type": "integer"
                    },
                    "values": {
                        "id": "/properties/test_case/items/properties/values",
                        "type": "array",
                        "length": { "$ref": "/properties/test_case/items/properties/k" },
                        "items": {
                            "id": "/properties/test_case/items/properties/values/items",
                            "type": "integer"
                        }
                    }
                }
            }
        }
    }
}








